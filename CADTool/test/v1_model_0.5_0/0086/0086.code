# height
<height>  3 

# define nodes
<node0>  (0 ,0 )
<node1>  (196 ,0 )
<node2>  (196 ,196 )
<node3>  (0 ,196 )
<node4>  (4 ,4 )
<node5>  (192 ,4 )
<node6>  (192 ,192 )
<node7>  (4 ,192 )

# draw face
<face> 
    <loop type="outer" >
