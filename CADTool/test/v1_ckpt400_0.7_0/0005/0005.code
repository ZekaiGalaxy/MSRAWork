# height
<height>  200 

# define nodes
<node0>  (-4 ,12 )
<node1>  (12 ,-100 )
<node2>  (31 ,-20 )
<node3>  (0 ,0 )
<node4>  (0 ,-10 )
<node5>  (-20 ,-10 )
<node6>  (-20 ,0 )
<node7>  (0 ,0 )
<node8>  (0 ,-10 )

# draw face
<face> 
    <loop type="outer" >
        <Line>  <node0>  <node1> 
        <Arc>  <node1>  <node4>  <node3>  <node2> 
        <Line>  <node5>  <node2> 
        <Arc>  <node0>  <node7>  <node6>  <node5> 
    </loop> 
    <loop type="inner" >
        <Circle> <node6>  <node8> 
    </loop> 
    <loop type="inner" >
        <Circle> <node3>  <node8> 
    </loop> 
</face>