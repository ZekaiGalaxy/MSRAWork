# height
<height>  200 

# define nodes
<node0>  (100 ,48 )
<node1>  (-100 ,-100 )
<node2>  (-100 ,100 )
<node3>  (100 ,100 )
<node4>  (0 ,0 )
<node5>  (40 ,0 )

# draw face
<face> 
    <loop type="outer" >
        <Line>  <node0>  <node1> 
        <Line>  <node1>  <node2> 
        <Line>  <node3>  <node2> 
        <Line>  <node0>  <node3> 
    </loop> 
    <loop type="inner" >
        <Circle> <node4>  <node5> 
    </loop> 
</face>